{
  "name": "jenko",
  "version": "0.0.1",
  "description": "An application for debugging React-Native App",
  "main": "index.js",
  "bin": {
    "jenko": "./src/cli/index.js"
  },
  "scripts": {
    "dev": "next",
    "serve": "node ./src/cli/index",
    "test": "jest --coverage",
    "format": "prettier-eslint --write \"pages/**/*.js\" && prettier-eslint --write \"src/**/*.js\" && prettier-eslint --write \"stories/**/*.js\"",
    "lint": "eslint ./src && eslint ./pages",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Marvin FRACHET, Alexandre GORY",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-css": "^0.1.5",
    "bulma": "^0.6.2",
    "commander": "^2.14.1",
    "express": "^4.16.2",
    "next": "^5.0.0",
    "object-sizeof": "^1.2.0",
    "opn": "^5.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rn-snoopy": "^2.0.2",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/addons": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "prettier": "^1.11.0",
    "prettier-eslint-cli": "^4.7.1"
  },
  "jest": {
    "setupFiles": [
      "./configs/setup/test.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\rn-snoopy.js$": "<rootDir>/__mocks__/rn-snoopy.js",
      "rn-snoopy/(.*)/buffer": "<rootDir>/__mocks__/buffer.js",
      "react-native/(.*)/EventEmitter": "<rootDir>/__mocks__/EventEmitter.js"
    }
  }
}
